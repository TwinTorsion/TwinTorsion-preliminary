{
	"name": "twintorsion-editor",
	"version": "1.0",
	"private": true,
	"scripts": {
		"dev": "set NODE_ENV=development && concurrently \"vite dev\" \"python ../server/app.py\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"start": "set NODE_ENV=production && node build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.3",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/lodash": "^4.17.7",
		"@types/prismjs": "^1.26.4",
		"@types/three": "^0.167.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-static": "^3.0.5",
		"@xyflow/svelte": "^0.1.13",
		"concurrently": "^9.0.1",
		"express": "^4.21.0",
		"html2pdf.js": "^0.10.2",
		"lodash": "^4.17.21",
		"prismjs": "^1.29.0",
		"svelte-jsoneditor": "^0.23.7",
		"svelte-portal": "^2.2.1",
		"three": "^0.167.1",
		"three-csg-ts": "^3.2.0"
	}
}
